		<control type="group">
			<left>$PARAM[group_left]</left>
			<visible>$PARAM[group_visible]</visible>
			<control type="image">
				<texture background="true" fallback="DefaultVideo.png">$PARAM[item_image]</texture>
				<include>FlixThumb</include>
			</control>
			<control type="image">
				<texture background="true" flipy="true" diffuse="thumbs/thumb_diffuse.png" fallback="DefaultVideo.png">$PARAM[item_image]</texture>
				<include>FlixThumbReflection</include>
			</control>
			<control type="image">
				<texture background="true">$PARAM[cover_image]</texture>
				<include>FlixDVDCoverVars</include>
			</control>
			<include>FlixThumbReflectionDVD</include>
			<control type="image">
				<include>WatchedOverlayShowCase</include>
				<texture>$PARAM[overlay_image]</texture>
			</control>
		</control>
	</include>
	<include name="FlixThumbReflectionDVD">
		<control type="image">
			<left>-27</left>
			<top>397</top>
			<width>325</width>
			<height>180</height>
			<colordiffuse>d1FFFFFF</colordiffuse>
			<animation effect="fade" start="70" end="70" time="0" condition="true">Conditional</animation>
			<visible>Skin.HasSetting(usecases) + Skin.HasSetting(clearcases) + [container.content(movies) | Container.Content(musicvideos)]</visible>
		</control>
	</include>
	<include name="FlixDVDCoverVars">
		<left>-1</left>
		<top>-8</top>
		<width>268</width>
		<height>402</height>
		<visible>Skin.HasSetting(usecases) + [container.content(movies) | Container.Content(musicvideos)]</visible>
	</include>
	<include name="FlixThumbReflectionDimensions">
		<left>19</left>
		<top>397</top>
		<width>228</width>
		<height>402</height>
	</include>
	<include name="FlixThumb">
		<left>-1</left>
		<top>-8</top>
		<width>268</width>
		<height>402</height>
		<align>center</align>
		<bordertexture border="20">thumbs/thumb_shadow_aa.png</bordertexture>
		<bordersize>20</bordersize>
	</include>
	<include name="FlixThumbReflectionMain">
		<control type="image">
			<texture background="true" flipy="true" diffuse="thumbs/thumb_diffuse.png" fallback="DefaultVideo.png">$VAR[ShowcaseItem0Var]</texture>
			<aspectratio scalediffuse="false" aligny="top">scale</aspectratio>
			<include>FlixThumbReflectionDimensions</include>
			<include>FlixModAnimation</include>
			<colordiffuse>52FFFFFF</colordiffuse>
		</control>
	</include>
	<include name="FlixThumbReflection">
		<aspectratio scalediffuse="false" aligny="top">scale</aspectratio>
		<include>FlixThumbReflectionDimensions</include>
		<colordiffuse>d1FFFFFF</colordiffuse>
		<animation effect="fade" start="70" end="70" time="0" condition="true">Conditional</animation>
	</include>
</includes>
